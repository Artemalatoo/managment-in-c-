import java.util.*;
class Main{
    public static boolean isPrime(int n){
        if(n%2==0 || n%3==0 || n%5==0){
            return false;
        }
        else{
            return true;
        }
    }
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        System.out.print(isPrime(n));
    }
}
